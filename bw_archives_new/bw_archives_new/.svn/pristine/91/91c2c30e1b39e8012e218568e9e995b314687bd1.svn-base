                                for(var i = 0, len = parameter_arr.length; i <= len -1; i++){
                                    tmp_arr = parameter_arr[i].split('=');
                                    $_GET[tmp_arr[0]] = decodeURIComponent(tmp_arr[1]);
                                }
                                return $_GET;
                            }
