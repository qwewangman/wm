*{
    margin: 0;
    padding:0;
    list-style: none;
    text-decoration: none;
    -webkit-appearance: none;
}
em,i{
    font-style: normal;
}
img{
    border:none;
    display: block;
}
html{
    height: 100%;
}
body {
    margin: 0;
    font-family:'Microsoft YaHei';
    margin:0 auto;
    font-size: 16px;
    background: #f4f6fa;
    min-height: 100%;
}

a:hover {
    text-decoration: none;
}
input,textarea,button{
    outline: none;
    -webkit-tap-highlight-color:rgba(0,0,0,0);
    border:none;
    background-color: rgba(0,0,0,0);
}
input::-webkit-input-placeholder, textarea::-webkit-input-placeholder {
    color: #999;
}
input:-moz-placeholder, textarea:-moz-placeholder {
    color: #999;
}
input::-moz-placeholder, textarea::-moz-placeholder {
    color: #999;
}
input:-ms-input-placeholder, textarea:-ms-input-placeholder {
    color: #999;
}
h1{
    font-style: normal;
}
.clear{
    clear: both;
    width: 0;
    height: 0;
    content: "";
}
.fl_left{
    float: left;
}
.fl_right{
    float: right;
}
.block{
    display: block;
}
.none{
    display: none;
}
/* headerbox */
.hederBox{
    width: 100%;
    padding:0 30px;
    box-sizing: border-box;
    height: 70px;
    line-height: 70px;
    border-bottom: 1px solid rgba(46,18,3,0.1);
    overflow: hidden;
    background: #fff;
}
.logo{
    text-indent: 52px;
    color: #222;
    font-size: 24px;
    background: url(../imgs/logo.png) left center no-repeat;
    background-size: 44px;
}
.user{
    color: #c7782e;
}
.signOut{
    color: #fff;
    width: 100px;
    height: 38px;
    line-height: 38px;
    border-radius: 19px;
    margin:16px 0 0 33px;
    text-indent: 47px;
    background: #f09c4e url(../imgs/loginout.png) left 23px center no-repeat;
    background-size: 17px;
    cursor: pointer;
}
/* contetnBox */
.contetnBox{
    overflow: hidden;
    height: 100%;
}
.sliderBox{
    width: 13%;
    background: #fff;
    padding-top: 8px;
    height: 100%;
}
.sliderList:hover{
    background: #fffaf0;
}
/*a:hover{*/
    /*color:red;*/
/*}*/

.sliderList{
    line-height: 50px;
    background-color: #fff;
    color: #333;
    display: block;
    overflow: hidden;
}
.sliderLists{
    background-color: #fffaf0;
    color: #c7782e;
}
.sliderOne{
    width: 4px;
    height: 40px;
    margin-top: 5px;
    background-color: #fff;
}
.sliderLists>.sliderOne{
    background-color: #c7782e;
}
.sliderTwo{
    width: 18px;
    margin-left: 20px;
    margin-right: 16px;
}
.rightBox{
    padding:0 30px;
    width: 87%;
    box-sizing: border-box;
}
.titleBox{
    overflow: hidden;
}
.title{
    line-height: 74px;
    color: #000;
    font-size: 24px;
}
.detaBox{
    margin-top: 20px;
}
.details{
    width: 100%;
    background-color: #fff;
}
/* page */
.tcdPageCode {
    padding: 15px 5px;
    text-align: left;
    color: #ccc;
    text-align: center;
}
.tcdPageCode a {
    display: inline-block;
    color: #c7782e;
    display: inline-block;
    height: 25px;
    line-height: 25px;
    padding: 0 5px;
    border: 1px solid #ddd;
    margin: 0 2px;
    border-radius: 4px;
    vertical-align: middle;
}
.tcdPageCode a:hover {
    text-decoration: none;
    border: 1px solid #c7782e;
}
.tcdPageCode span.current {
    font-size: 12px;
    display: inline-block;
    height: 25px;
    line-height: 25px;
    padding: 0 5px;
    margin: 0 2px;
    color: #fff;
    background-color: #c7782e;
    border: 1px solid #c7782e;
    border-radius: 4px;
    vertical-align: middle;
}
.tcdPageCode span.disabled {
    font-size: 12px;
    display: inline-block;
    height: 25px;
    line-height: 25px;
    padding: 0 5px;
    margin: 0 2px;
    color: #bfbfbf;
    background: #f2f2f2;
    border: 1px solid #bfbfbf;
    border-radius: 4px;
    vertical-align: middle;
}
.tcdPageCode .tcdNumber{
    width: 25px;
    text-align: center;
    padding: 0;
}
.tcdPageCode span.current{
    width: 25px;
    text-align: center;
    padding: 0;
}
