    public function array_unset_tt($arr,$key){  //二维数组根据某字段去重
        $res = array();
        foreach ($arr as $value) {
            if(isset($res[$value[$key]])){
                unset($value[$key]);
            }
            else{
                $res[$value[$key]] = $value;
            }
        }
        return $res;
    }

    public function numeral($key){
        switch ($key){
            case 1: $key = '一';
                break;
            case 2: $key = '二';
                break;
            case 3: $key = '三';
                break;
            case 4: $key = '四';
                break;
            case 5: $key = '五';
                break;
            case 6: $key = '六';
                break;
            case 7: $key = '七';
                break;
            case 8: $key = '八';
                break;
            case 9: $key = '九';
                break;
            case 10: $key = '十';
                break;
        }
        return $key;
    }







