/* alertBox */
.alertBox{
    position: fixed;
    top:0;
    bottom: 0;
    left:0;
    right: 0;
    background-color: rgba(0,0,0,0.6);
    display: -webkit-flex; /* Safari */
    display: flex;
    flex-wrap:wrap;
    justify-content: center;
    align-items: center;
    display: none;
}
.alert{
    width: 662px;
    background-color: #fff;
    padding-bottom: 35px;
}
.alertTitle{
    background-color: #f5f5f5;
    line-height: 50px;
    text-indent: 20px;
    color: #333;
}
.alertList{
    display: -webkit-flex; /* Safari */
    display: flex;
    flex-wrap:wrap;
    justify-content: flex-start;
    align-items: flex-start;
    margin-top: 30px;
}
.alertName{
    width: 120px;
    text-align: right;
    color: #666;
    line-height: 38px;
}
.alertInput{
    width: 450px;
    height: 36px;
    background-color: #ffffff;
    border: solid 1px #e7e7eb;
    margin-left: 20px;
}
.alertInput>input{
    display: block;
    width: 100%;
    height: 100%;
    text-indent: 8px;
    color: #000;
}
.alertInput>.alertText{
    width: 100%;
    height: 100%;
    text-indent: 8px;
    color: #000;
    line-height: 36px;
}
.alertInputs{
    position: relative;
}
.alertSelect{
    position: absolute;
    top:37px;
    left: -1px;
    border: solid 1px #e7e7eb;
    width:450px;
    background-color: #fff;
    display: none;
    z-index: 99;
}
.alertSelList{
    line-height: 36px;
    text-indent: 8px;
    width: 100%;
}
.alertSelList:hover{
    background-color: #f0fafa;
}
.alertText{
    background: url(../imgs/shape_sanjiao_g.png) right 14px center no-repeat;
    background-size: 14px;
}
.upLoad{
    width: 117px;
    text-indent: 57px;
    color: #666;
    background: url(../imgs/upload.png) left 29px center no-repeat;
    background-size: 16px;
    line-height: 36px;
}
.noBorder{
    border:0;
    overflow: hidden;
}
.alertBtn{
    width: 120px;
    height: 36px;
    box-sizing: border-box;
    text-align: center;
    line-height: 36px;
    border-radius: 4px;
}
.sure{
    background-color: #c7782e;
    color: #fff;
}
.reset{
    color: #333;
    border:1px solid #c7782e;
    margin-left: 28px;
}
/*
    修改样式
*/
.newRightBox{
    background: #fff;
    width: 100%;
    padding: 50px 0 20px ;
}
.layui-btn-primary {
    border: 1px solid #C9C9C9;
    background-color: #fff;
    color: #555;
}
.layui-btn-primary:hover {
    border-color: #f09c4e;
    color: #333;
}
.layui-form-radio>i:hover, .layui-form-radioed>i{
    color: #f09c4e;
}
.layui-laypage .layui-laypage-curr .layui-laypage-em{
    background-color: #f09c4e;
}
.layui-form-checked[lay-skin=primary] i {
    border-color: #f09c4e;
    background-color: #f09c4e;
    color: #fff;
}
.layui-form-checkbox[lay-skin=primary]:hover i{
    border-color: #f09c4e;
}