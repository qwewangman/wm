<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>北京舞蹈学院学生档案管理系统</title>
    <meta name="keywords" content="">
    <meta name="description" content="基于Bootstrap3最新版本开发的扁平化主题，她采用了主流的左右两栏式布局，使用了Html5+CSS3等现代技术">

    <link rel="shortcut icon" href="favicon.ico"> <link href="__STATIC__/css/bootstrap.min.css?v=3.3.5" rel="stylesheet">
    <link href="__STATIC__/css/font-awesome.min.css?v=4.4.0" rel="stylesheet">
    <link href="__STATIC__/css/animate.min.css" rel="stylesheet">
    <link href="__STATIC__/css/style.min.css?v=4.0.0" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="__STATIC__/css/records.css">
    <link rel="stylesheet" type="text/css" href="__STATIC__/css/details.css">
    <link rel="stylesheet" type="text/css" href="__STATIC__/font/iconfont.css">
    <link rel="stylesheet" type="text/css" href="__STATIC__/layui/css/layui.css">
    <link rel="stylesheet" type="text/css" href="__STATIC__/css/style.css">

    <script src="__STATIC__/js/jquery-2.1.1.min.js"></script>
    <script src="__STATIC__/layui/layui.js" charset="utf-8"></script>
    <script src="__STATIC__/js/index.js"></script>

</head>
<style>
    .btn-primary{
        border-color: #f09c4e;
        background-color: #f09c4e;
    }
    .btn-primary:hover{
        background-color: #f09c4e;
        border-color: #18a689;
    }
</style>
<body class="gray-bg">
<div class="hederBox">
    <div class="fl_left logo">北京舞蹈学院学生档案管理系统</div>
    <div class="fl_right">
        <div class="fl_left user">管理员</div>
        <a href="{:url('/login_out')}"  style="color: white"><div class="fl_right signOut">退出</div></a>
        <div class="clear"></div>
    </div>
    <div class="clear"></div>
</div>
<div class="layui-row layui-col-space15">
    {include file='layout'}
    <div class="titleBox">
        <div class="title fl_left">详情</div>
        <a href="javascript:history.back(-1)"><div class="fl_right" style="width: 110px;height: 34px;border: solid 1px #666666;line-height: 34px;text-align: center;margin-top: 20px;color: #333;">返回</div></a>
        <div class="clear"></div>
    </div>
    <div class="layui-col-md4">
        <div class="layui-card">
            <div class="layui-card-header">个人资料</div>
            <div class="layui-card-body">
                <div style="padding-left: 10px;">
                    <img alt="" class="img-responsive" src="__STATIC__{$info.stu_photo}">
                </div>
                <h4><strong>{$info.user_name}</strong>&nbsp&nbsp<strong>{$info.sex}</strong></h4>
                <p><i class="fa fa-map-marker"></i>{$info.stu_address != '' ? $info.stu_address : '未填写住址'}</p>
                <h5>
                    关于我
                </h5>
                <div class="row">
                    <div class="col-sm-6">
                        <p>
                            卷号: {$info.file_number}<br>
                            学号: {$info.sc_number}<br>
                            年级: {$info.grade}<br>
                            班级: {$info.class}<br>
                            院系: {$info.courtyard}<br>
                            专业: {$info.major}<br>
                            身份证号: {$info.card}<br>
                            存放位置: {$info.file_derice}<br>
                            电话号:<br> {foreach name='phone' item='v'}
                            {$v}<br>
                            {/foreach}
                        </p>
                    </div>
                    <div class="col-sm-6">
                        <p>
                            学历: {$info.record}<br>
                            入档号: {$info.filing_number}<br>
                            案卷状态: {$info.status}<br>
                            民族: {$info.nation}<br>
                            国籍: {$info.country}<br>
                            立卷日期: {$info.addtime}<br>
                            录取类别: {$info.student_type}<br>
                            卷内文件状态: {$info.stu_within}<br>
                            备注: {$info.content}<br>
                            {$stu_change_info_join.addtime}
                        </p>
                    </div>
                </div><br>
                <div class="row">
                    <div class="col-sm-4">
                        <span class="bar"></span>
                        <h5><strong>{$num.file}</strong> 材料:</h5>
                    </div>
                    <div class="col-sm-4">
                        <span class="line"></span>
                        <h5><strong>{$num.change}</strong> 动态:</h5>
                    </div>
                </div>
                <input type="hidden" value="{$info.status == '未在档' ? 0 : 1}" id="student_type">
                <input type="hidden" value="{$info.stu_within == '暂未接收' ? 0 : 1}" id="within_type">
                <input type="hidden" value="{$info.id}" id="user_id">
                <div class="user-button">
                    <div class="row">
                        <div class="col-sm-4">
                            <a href="{:url('/file_show')}?id={$info.id}"><button type="button" id="shift_out" class="btn btn-primary btn-sm btn-block"><i class="fa fa-envelope"></i> 查看文件</button></a>
                        </div>
                        <div class="col-sm-4">
                            <a href="{:url('/details')}?id={$info.id}"><button type="button" id="receive" class="btn btn-default btn-sm btn-block" disabled><i class="fa fa-coffee"></i> 文件接收</button></a>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <div class="layui-col-md6">
        <div class="layui-card">
            <div class="layui-card-header">变动记录</div>
            <div class="layui-card-body">
                <table class="layui-table"  id="LAY_table_user"  lay-data="{ url:'', page:true, id:'idTest'}" lay-filter="demo">
                </table>
            </div>
        </div>
    </div>
</div>
